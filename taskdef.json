{
   "containerDefinitions": [
      {
         "essential": true,
         "image": "<IMAGE1_NAME>",
         "name": "pheonix",
         "portMappings": [
            {
               "containerPort": 80,
               "hostPort": 80,
               "protocol": "tcp"
            }
         ]
      }
   ],
   "cpu": "256",
   "executionRoleArn": "arn:aws:iam::307533935679:role/ecsTaskExecutionRole",
   "family": "pheonix",
   "memory": "512",
   "networkMode": "awsvpc",
   "requiresCompatibilities": [
       "FARGATE"
    ]
}
